# ==========================================
# ESSENTIAL ENVIRONMENT VARIABLES
# For oncobot Eligibility Funnel System
# ==========================================

# ------------------------------------------
# CRM & Lead Management (Required for lead capture)
# ------------------------------------------
GHL_WEBHOOK_URL=your_gohighlevel_webhook_url
GHL_INTEGRATION_TOKEN=your_gohighlevel_v2_integration_token
GHL_LOCATION_ID=your_gohighlevel_location_id

# ------------------------------------------
# Conversion Tracking (Required for marketing)
# ------------------------------------------
# Google Ads
NEXT_PUBLIC_GOOGLE_ADS_CONVERSION_ID=your_conversion_id
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id

# Facebook Pixel
NEXT_PUBLIC_FACEBOOK_PIXEL_ID=your_pixel_id

# ------------------------------------------
# Database (Required)
# ------------------------------------------
DATABASE_URL=your_database_url_here

# ------------------------------------------
# Authentication (Required)
# ------------------------------------------
BETTER_AUTH_SECRET=your_secret_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# ------------------------------------------
# Email (Required for magic links & contact forms)
# ------------------------------------------
RESEND_API_KEY=your_resend_api_key_here
EMAIL_FROM=oncobot <noreply@onco.bot>
# Where to send contact form submissions
CONTACT_EMAIL_TO=support@onco.bot

# ------------------------------------------
# Application URLs (Required)
# ------------------------------------------
NEXT_PUBLIC_APP_URL=https://onco.bot
NEXT_PUBLIC_API_URL=https://api.onco.bot

# ------------------------------------------
# Analytics (Optional but recommended)
# ------------------------------------------
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://your-posthog-host.com

# ------------------------------------------
# Logging Configuration (Optional)
# ------------------------------------------
# Options: TRACE, DEBUG, INFO (default), WARN, ERROR, SILENT
LOG_LEVEL=INFO

# ------------------------------------------
# AI Services (Required for core functionality)
# ------------------------------------------
# At least one AI provider is required
OPENAI_API_KEY=your_openai_api_key_here
# Or alternative providers:
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# XAI_API_KEY=your_xai_api_key_here
# GROQ_API_KEY=your_groq_api_key_here
# GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_here

# ------------------------------------------
# Storage & Caching (Required for production)
# ------------------------------------------
REDIS_URL=your_redis_url_here
BLOB_READ_WRITE_TOKEN=your_blob_token_here

# ------------------------------------------
# Security (Required for production)
# ------------------------------------------
CRON_SECRET=your_cron_secret_here
ALLOWED_ORIGINS=http://localhost:3000,https://onco.bot,https://www.onco.bot

