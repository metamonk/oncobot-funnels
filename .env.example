# Development & Sandbox
DAYTONA_API_KEY=your_daytona_api_key_here

# Logging Configuration
# LOG_LEVEL: Controls verbosity of server logs
# Options: TRACE, DEBUG, INFO (default), WARN, ERROR, SILENT
# - TRACE: Everything including DB queries and request details
# - DEBUG: Detailed debugging information
# - INFO: Standard operational logs (recommended for production)
# - WARN: Only warnings and errors
# - ERROR: Only error messages
# - SILENT: No logging
LOG_LEVEL=INFO

# Database & Storage
DATABASE_URL=your_database_url_here
REDIS_URL=your_redis_url_here
BLOB_READ_WRITE_TOKEN=your_blob_token_here

# Authentication
BETTER_AUTH_SECRET=your_secret_key_here
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
